
public class BubbleSort
{
	public static void main(String[] args) {
	    int[] a = new int[]{13,46,24,52,20,9};
	    int n=6;
	    
	    for(int i=n-1;i>=0;i--){
	        for(int j=0;j<i;j++){
	            if(a[j]>a[j+1]){
	                int temp=a[j];
	                a[j]=a[j+1];
	                a[j+1]=temp;
	            }
	        }
	    }
	    
	    for(int i=0;i<n;i++){
	        System.out.print(a[i] + " ");
	    }
	}
}
